
<div class="container">
  <div class="d-flex justify-content-center"><img src="https://st4.depositphotos.com/9904426/27502/i/450/depositphotos_275022004-stock-photo-creative-concept-beer-glasses-on.jpg"></div>
  <div class="d-flex justify-content-center"><h1>Sign in, Friend </h1></div>
  <div class="d-flex justify-content-center">
      <%= form_with url: session_path, method: :post do |form| %>
      <p> username <%= form.text_field :username %> </p>
      <p> password<%= form.password_field :password %> </p>
  <div class="d-flex justify-content-center"><%= form.submit "Log in" %></div>
  </div>
  <% end %>
</div>
